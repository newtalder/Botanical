name = input("What's your name?\n")
move = ("")
while move != q:
	move = input("Where would you like to move? Your options are n, e, s, or w. Enter q to leave.\n")
	if move == "n":
		print("You have moved north.")
	elif move == "e":
		print("You have moved east.")
	elif move == "s":
		print("You have moved south.")
	elif move == "w":
		print("You have moved west.")
	else:
		print("Sorry, that's not a place to move!")